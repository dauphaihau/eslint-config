{
  "name": "dauphaihau-eslint-config-monorepo",
  "private": true,
  "packageManager": "pnpm@9",
  "scripts": {
    "build": "pnpm -r --filter @dauphaihau/eslint-config build",
    "dev": "pnpm -r --filter @dauphaihau/eslint-config dev",
    "lint:playground": "pnpm --filter ts lint && pnpm --filter js lint",
    "fix:playground": "pnpm --filter ts fix && pnpm --filter js fix",
    "lint:ts": "pnpm --filter ts lint",
    "lint:js": "pnpm --filter js lint",
    "lint:react": "pnpm --filter react lint",
    "fix:js": "pnpm --filter js fix",
    "fix:ts": "pnpm --filter ts fix",
    "fix:react": "pnpm --filter react fix",
    "release:patch": "pnpm --filter @dauphaihau/eslint-config release:patch",
    "release:minor": "pnpm --filter @dauphaihau/eslint-config release:minor",
    "release:major": "pnpm --filter @dauphaihau/eslint-config release:major"
  }
}