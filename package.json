{
  "name": "@dauphaihau/eslint-config",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm exec tsup src/index.ts --dts --format esm --external @eslint/js --external @stylistic/eslint-plugin --external typescript-eslint --external eslint-plugin-filenames-simple",
    "prepublishOnly": "pnpm build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "release:patch": "pnpm build && pnpm version patch && pnpm publish",
    "release:minor": "pnpm build && pnpm version minor && pnpm publish",
    "release:major": "pnpm build && pnpm version major && pnpm publish"
  },
  "peerDependencies": {
    "eslint": "^9.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "typescript-eslint": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@stylistic/eslint-plugin": "^1.6.0",
    "eslint": "^9.0.0",
    "eslint-plugin-filenames-simple": "^0.9.0",
    "globals": "^16.5.0",
    "semantic-release": "^21.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "typescript-eslint": "^8.0.0",
    "jiti": "^2.0.0"
  }
}